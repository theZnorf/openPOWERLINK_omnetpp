%description:
Run the singleCn simulation and check if any errors occur

%testprog: opp_run

%inifile: omnetpp.ini
[General]
network = openpowerlink.singleCn
debug-on-errors = false
ned-path = .;../../../src
tkenv-image-path = ../../../images

user-interface = Cmdenv
cmdenv-express-mode = true

singleCn.libraryPath = "../../../../openPOWERLINK_V2/stack/lib/linux/x86_64/"
singleCn.mn.demo.cdcFilename = "../../../mnobd.cdc"

singleCn.mn.demo.startUpDelay = 0s
singleCn.cn.demo.startUpDelay = 3s
singleCn.cn.demo.shutdownTime = 15s
singleCn.mn.demo.shutdownTime = 16s

#%contains: results/General-0.elog
#finished
