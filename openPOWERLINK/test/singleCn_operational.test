%description:
Run the singleCn simulation and check if either nodes reach operational state

%testprog: opp_run

%inifile: omnetpp.ini
[General]
network = openpowerlink.singleCn
debug-on-errors = false
ned-path = .;../../../src
tkenv-image-path = ../../../images

user-interface = Cmdenv
cmdenv-express-mode = true

singleCn.libraryPath = "../../../../openPOWERLINK_V2/stack/lib/linux/x86_64/"
singleCn.mn.demo.cdcFilename = "../../../mnobd.cdc"

singleCn.mn.demo.startUpDelay = 0s
singleCn.cn.demo.startUpDelay = 3s
singleCn.cn.demo.shutdownTime = 15s
singleCn.mn.demo.shutdownTime = 16s

%postrun-command: scavetool v -p "module(**cn**) AND name(**nmtStates**)" -O cnNmtStates results/General-0.vec
%postrun-command: scavetool v -p "module(**mn**) AND name(**nmtStates**)" -O mnNmtStates results/General-0.vec

%file-exists: cnNmtStates.vec
%file-exists: mnNmtStates.vec

%contains: cnNmtStates.vec
509

%contains: mnNmtStates.vec
765
